<div class="modal fade" id="importToolboxModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">
                    {{ 'application.project.import_toolbox_title'|trans({},'application') }}
                </h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                {{ form_start(importToolboxForm, {'action': url('app_project_import_toolbox')}) }}
                {{ form_row(importToolboxForm.toolboxFile) }}
                {{ form_row(importToolboxForm.autoValidate) }}
                <button class="btn btn-primary btn-sm"><i class="far fa-save"></i>&nbsp; {{ button_label|default('application.project.import_toolbox_submit'|trans({},'application')) }}</button>
                {{ form_end(importToolboxForm) }}
            </div>
        </div>
    </div>
</div>